            android:alpha="1"/>
    </item>
    <!-- Left arm -->
    <item
        android:left="-35dp"
        android:bottom="112dp">
        <bitmap
            android:src="@drawable/woman12_arm_left"
            android:gravity="bottom"
            android:alpha="1"/>
    </item>
    <!-- Right arm -->
    <item
        android:left="35dp"
        android:bottom="112dp">
        <bitmap
            android:src="@drawable/woman12_arm_right"
            android:gravity="bottom"
            android:alpha="1"/>
    </item>
    <!-- Left leg -->
    <item
        android:left="-18dp"
        android:bottom="48dp">
        <bitmap
            android:src="@drawable/woman12_leg_left"
            android:gravity="bottom"
            android:alpha="1"/>
